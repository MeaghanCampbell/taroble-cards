{
  "name": "taroble-cards",
  "version": "1.0.0",
  "description": "Tarot readings with attitude",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeaghanCampbell/taroble-cards.git"
  },
  "author": "J. Scott Rumptz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MeaghanCampbell/taroble-cards/issues"
  },
  "homepage": "https://github.com/MeaghanCampbell/taroble-cards#readme",
  "dependencies": {
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.2"
  }
}
